#!/bin/sh

SRCPATCH=/tmp/src/patch
ETCHOME=$HOME/broker/etc

if [ -d ${SRCBIN} ]; then
  echo "############ In Bin Copeying : ) ############"

chmod +x ${SRCPATCH}/*
mkdir $ETCHOME
cp -v ${SRCPATCH}/* $ETCHOME/

cp -v /tmp/src/patch/configure_s2i_files.sh $AMQ_HOME/bin/configure_s2i_files.sh

fi


exit 0
